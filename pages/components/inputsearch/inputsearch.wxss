/* pages/components/inputsearch/inputsearch.wxss */
.page-inputsearch {
  width: 90%;
  height: 280px;
  display: flex;
  flex-direction: column;
  position: absolute;
  top: 200px;
  left: 50%;
  transform:translate(-50%,-50%);
  background-color: #eee;
  z-index: 999;
   overflow: scroll;
}
page-inputsearch ul {
  /* height: 1000px; */
  display: flex;
  flex-direction: column;
  
}
page-inputsearch li{
 height: auto;

   padding: 0 5px;
  display: flex;
  flex-wrap: nowrap;
  flex-direction: row;
}
li.name {
  display: flex;
  flex-direction: row;
}
.close {
  height: 40px;
  width: 40px;
  /* line-height: 40px; */
  text-align: center;
  position: absolute;
  top: 1px;
  right: 1px;
  z-index: 9999;
}
.nomsg {
  font-size: 24px;
  font-weight: bold;
}